   <EventFiltering>
 <RuleGroup name="" groupRelation="or">
      <FileCreateTime onmatch="include">
	  	<Image name="technique_id=T1099,technique_name=Timestomp" condition="begin with">C:\Temp</Image><!--Look for timestomping in temp folders-->
